@model List<string>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Produtos</title>
</head>
<body>
    <div> 
        <ul>
            @foreach(string produto in Model)
            {
                <li>
                    <b>@produto</b>
                </li>
            }
        </ul>
    </div>
</body>
</html>
